<ivysettings>
    <settings defaultResolver="central" />
    <resolvers>
        <ibiblio name="central" m2compatible="true" />
        <filesystem name="local">
            <artifact pattern="${ivy.settings.dir}/lib/[artifact]-[revision].[ext]" />
        </filesystem>
    </resolvers>
    <modules>
        <module organisation="com.microsoft" resolver="local" />
    </modules>
</ivysettings>
